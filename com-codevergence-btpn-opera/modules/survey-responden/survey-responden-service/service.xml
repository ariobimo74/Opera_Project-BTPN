<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_2_0.dtd">

<service-builder dependency-injector="spring" package-path="responden">
	<namespace>Responden</namespace>
	<!--<entity data-source="sampleDataSource" local-service="true" name="Foo" remote-service="false" session-factory="sampleSessionFactory" table="foo" tx-manager="sampleTransactionManager uuid="true"">-->
	<entity table="survey_responden" local-service="true" name="SurveyResponden" remote-service="true" uuid="false" data-source="SurveyResponden">

		<!-- PK fields -->

		<column db-name="id" name="id" primary="true" type="long" />

		<!-- Audit fields -->

		<column db-name="nik" name="nik" type="String" />
		<column db-name="survey_id" name="surveyId" type="long" />
		<column db-name="answer1" name="answer1" type="Double" />
		<column db-name="answer2" name="answer2" type="Double" />
		<column db-name="answer3" name="answer3" type="Double" />
		<column db-name="answer4" name="answer4" type="Double" />
		<column db-name="answer5" name="answer5" type="Double" />
		<column db-name="answer6" name="answer6" type="Double" />
		<column db-name="answer7" name="answer7" type="Double" />
		<column db-name="answer8" name="answer8" type="Double" />
		<column db-name="answer9" name="answer9" type="Double" />
		<column db-name="answer10" name="answer10" type="Double" />

		<!-- Other fields -->

		<column db-name="note" name="note" type="String" />
		<column db-name="status" name="status" type="String" />
		<column db-name="submitted_date" name="submittedDate" type="Date" />

		<!-- Order -->

		<order by="asc">
			<order-column name="id" />
		</order>

		<!-- Finder methods -->

		<finder name="NIK" return-type="Collection">
			<finder-column name="nik" />
		</finder>
		<finder name="SurveyId" return-type="Collection">
			<finder-column name="surveyId"/>
		</finder>
	</entity>
</service-builder>